# Copy this file to .env.local and fill in your own credentials and secrets

# --- Database ---
DATABASE_URL="postgresql://<username>:<password>@localhost:5432/saas_app?schema=public"

# --- NextAuth Configuration ---
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="<generate_with: openssl rand -base64 48>"

# --- JWT ---
JWT_SECRET="<generate_with: openssl rand -base64 48>"

# --- AES-GCM Encryption ---
ENCRYPTION_KEY="<generate_with: openssl rand -base64 32>"

# --- Misc ---
NODE_ENV="development"
NEXT_PUBLIC_APP_ENV="development"
